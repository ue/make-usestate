{
	"name": "state-to-usestate",
	"author": "ue",
	"displayName": "Convert state to useState",
	"description": "easily convert class state to useState",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.0.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.stateToUseState"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "extension.stateToUseState",
			"title": "State To useState"
		}],
		"keybindings": [
      {
        "command": "extension.stateToUseState",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "editorTextFocus"
      }
    ]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
